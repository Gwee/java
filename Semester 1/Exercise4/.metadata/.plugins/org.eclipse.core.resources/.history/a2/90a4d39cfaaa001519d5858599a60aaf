import java.util.Scanner;

public class Program {
	public static void main (String[] args){
		System.out.println(Calculator.approximateTwo(4));
		System.out.println(Calculator.approximateE(4));
		int n;
		int counter=0;
		int arr[] = new int[10];
		Scanner scan = new Scanner(System.in);
		while (counter <arr.length-1) {
			n = scan.nextInt();
			arr[counter]= n;
			counter++;
		}
	}
}
